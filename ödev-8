3-)VERİLERİ GÜNVELLEMEK İÇİN ÖRNEKLER;

UPDATE employee
SET first_name = 'Yagmur Gungoz'
birthday = '1998-01-23'
email = 'yagmur@gungoz.com'
WHERE id = 21;

UPDATE employee
SET first_name = 'Ronaldo'
birthday = '1988-06-06'
email = 'siuu@uefa.com'
WHERE id = 33;

UPDATE employee
SET email = 'example@com.tr'
WHERE email LIKE '%.gov'
RETURNING *;

UPDATE employee
SET first_name = 'Yusuf'
WHERE name = 'Joseph'
RETURNING *;

UPDATE employee
SET first_name = 'Example'
birthday = '2024-02-06'
WHERE id BETWEEN 35 AND 45 ;

4-)VERİLERİ SİLMEK İÇİN ÖRNEKLER;

DELETE FROM employee 
where id = 6
RETURNING * ;

DELETE FROM employee
WHERE first_name ILIKE '%a%a%'	
RETURNING *;

DELETE FROM employee
WHERE id>48;

DELETE FROM employee
WHERE 
	(id>18 AND id<28) OR		
	(email LIKE '%.cz')
RETURNING *;

DELETE FROM employee
WHERE id =2;	
